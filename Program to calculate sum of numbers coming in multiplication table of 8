#include <stdio.h>

int main() {

    int num;
    int i;

    printf("Enter the number for wich you want its table\n");
    scanf("%d", &num);

    for ( i = 1; i <=10; i++)
    {
        printf("%d*%d = %d\n", num , i , i*num);
    }

    printf("The sum of the multiplication table of 8 is%d");

    
    
    return 0;

}
